{
  "family": "sumit-ecs-task",
  "containerDefinitions": [
    {
      "name": "sumit-container",
      "image": "<IMAGE_URI>",
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
